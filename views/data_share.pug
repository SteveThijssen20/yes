        main.main-content
          section.settings
            .settings__header
              h2 Data
              ul.settings__tabs
                li.settings__tabs-item.active(data-tab='1') Data Uploading
                li.settings__tabs-item(data-tab='2') User list
            .settings__content
              #tab-1.settings__content-item.settings__profile.active
                form.form.settings-form
                  .form__row
                    .form__col-lg
                      label.form__label
                        | User name
                      .input-wrap
                        input#username(type='text', value='')
                    .form__col-lg
                      label.form__label
                        | First name
                      .input-wrap
                        input#firstname(type='text', value='')
                    .form__col-lg
                      label.form__label
                        | Last name
                      .input-wrap
                        input#lastname(type='text', value='')
                  .form__row
                    .form__col-lg
                      label.form__label
                        | Email
                      .input-wrap
                        input#email(type='text', value='')
                    .form__col-lg
                      label.form__label
                        | Gender
                      select#gender.custom-select
                        option(value='male') male
                        option(value='female') female
                    .form__col-lg
                      label.form__label
                        | Phone
                      .input-wrap
                        input#phone(type='text', value='')
                  button#submit.btn.btn--green Save settings
                  p     
                  p
                  #transaction_expiration.form__row
                  p
                  #transaction_ref_block_num.form__row
                  p
                  #transaction_ref_block_prefix.form__row
                  p
                  #transaction_data.form__row
                  p
                  #transaction_signatures.form__row
                  p
              #tab-2.settings__content-item.settings__security
                .security__row
                  button#data_output.btn.btn--green Show list from node
                .security__row
                  #container(style='width: 100%')
    .d-none
      form#add-wallet.popup.popup-add-wallet.zoom-anim-dialog
        .popup__content
          img.popup-add-wallet__img(src='dash/img/popup-add-wallet-icon.png', alt='')
          h3 Add new wallet
          .popup-add-wallet__row
            .input-wrap
              input(type='text', placeholder='Name wallet')
            select.custom-select
              option(value='Bitcoin') Bitcoin
              option(value='Ethereum') Ethereum
              option(value='Litecoin') Litecoin
          button.btn.btn--blue.btn--full Add new wallet
        .popup__thanks
          img(src='dash/img/popup-success.png', alt='')
          h3 Your wallet added successful!
